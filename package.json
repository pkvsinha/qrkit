{
  "name": "@pkvsinha/symbol-codec",
  "version": "0.0.1",
  "license": "MIT",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "wasm/pkg",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/umd/symbol-codec.min.js",
  "jsdelivr": "dist/umd/symbol-codec.min.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "source": "./src/index.ts",
      "default": "./dist/esm/index.mjs"
    },
    "./render": {
      "types": "./dist/types/render/index.d.ts",
      "import": "./dist/esm/render/index.mjs",
      "require": "./dist/cjs/render/index.cjs",
      "source": "./src/render/svg.ts"
    },
    "./helpers": {
      "types": "./dist/types/helpers/index.d.ts",
      "import": "./dist/esm/helpers/index.mjs",
      "require": "./dist/cjs/helpers/index.cjs",
      "source": "./src/helpers/index.ts"
    },
    "./wasm": {
      "types": "./dist/types/wasm/index.d.ts",
      "import": "./dist/esm/wasm/index.mjs",
      "require": "./dist/cjs/wasm/index.cjs",
      "source": "./src/wasm/index.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist || rm -rf dist",
    "examples": "vite exmaples",
    "test:int": "http-server ./ -p 2467",
    "build": "npm run clean && tsup --config tsup.config.ts && npm run types",
    "types": "tsc -p tsconfig.build-types.json",
    "build:rollup": "rollup -c",
    "dev": "vite examples",
    "bench": "node --loader ts-node/esm scripts/bench.ts || node scripts/bench.js",
    "size-check": "node scripts/size-check.js",
    "prepublishOnly": "npm run clean && npm run types && npm run build && npm run size-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "http-server": "^14.1.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.0"
  }
}
