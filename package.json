{
  "name": "@pkvsinha/qrkit",
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./render": {
      "types": "./dist/render/index.d.ts",
      "import": "./dist/render/index.js"
    },
    "./wasm": {
      "types": "./dist/wasm/index.d.ts",
      "import": "./dist/wasm/index.js"
    }
  },
  "files": [
    "dist",
    "wasm/pkg"
  ],
  "scripts": {
    "build": "tsup --config build.config.ts && npm run build:wasm",
    "build:wasm": "cd wasm && wasm-pack build --target web --out-dir pkg",
    "dev": "vite examples",
    "typecheck": "tsc --noEmit",
    "bench": "node dist/bench.js"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vite": "^5.4.0"
  }
}
